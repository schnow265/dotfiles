{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#308898",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "in ",
          "type": "text"
        },
        {
          "foreground": "#0d65ae",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#00d7d7",
          "style": "plain",
          "template": "‚ùÑ   ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
